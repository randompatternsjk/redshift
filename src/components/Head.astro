---
import { ClientRouter } from "astro:transitions";
import Nav from "./Nav.astro";
---
<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
<meta name="viewport" content="width=device-width, initial-scale=1" />
<meta name="generator" content={Astro.generator} />
<ClientRouter />
<Nav />

<section class="content-center my-12">
    <div class="flex flex-col justify-start w-full max-w-md mx-auto my-auto gap-4">
        <img
          src="/svgs/theokaylakes.svg"
          alt="The Okay Lakes Logo"
          width="300"
          height="80"
          class="object-contain w-full max-w-[400px] mx-auto"
          loading="eager"
        />
        <img
          src="/svgs/redshift.svg"
          alt="Redshift Logo"
          width="400"
          height="80"
          class="object-contain w-full max-w-[400px] mx-auto"
          loading="eager"
        />
    </div>
</section>